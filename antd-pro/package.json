{
  "name": "gadmins-antd-pro",
  "version": "1.0.0",
  "private": true,
  "description": "An out-of-box UI solution for enterprise applications",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "umi build",
    "deploy": "npm run site && npm run gh-pages",
    "dev": "npm run start:dev",
    "fetch:blocks": "pro fetch-blocks --branch antd@4 && npm run prettier",
    "format-imports": "cross-env import-sort --write '**/*.{js,jsx,ts,tsx}'",
    "gh-pages": "cp CNAME ./dist/ && gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "check-prettier lint",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"**/*\"",
    "start": "umi dev",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none umi dev",
    "start:no-mock": "cross-env MOCK=none umi dev",
    "start:no-ui": "cross-env UMI_UI=none umi dev",
    "start:pre": "cross-env REACT_APP_ENV=pre umi dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none umi dev",
    "test": "umi test",
    "test:all": "node ./tests/run-tests.js",
    "test:component": "umi test ./src/components",
    "tsc": "tsc",
    "ui": "umi ui"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.0.6",
    "@ant-design/pro-layout": "^5.0.10",
    "@ant-design/pro-table": "2.2.3",
    "@antv/data-set": "^0.11.4",
    "@ckeditor/ckeditor5-build-classic": "^18.0.0",
    "@ckeditor/ckeditor5-react": "^2.1.0",
    "@formily/antd": "^1.1.2",
    "@formily/antd-components": "^1.1.2",
    "ace-builds": "^1.4.11",
    "antd": "^4.1.5",
    "classnames": "^2.2.6",
    "copy-to-clipboard": "^3.3.1",
    "crypto-js": "^4.0.0",
    "dva": "^2.6.0-beta.20",
    "immutability-helper": "^3.0.2",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "omit.js": "^1.0.2",
    "path-to-regexp": "6.1.0",
    "qs": "^6.9.3",
    "react": "^16.13.1",
    "react-ace": "^8.1.0",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dnd": "^10.0.2",
    "react-dnd-html5-backend": "^10.0.2",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "react-json-view": "^1.19.1",
    "redux": "^4.0.5",
    "umi": "^3.1.2",
    "umi-request": "^1.2.19",
    "use-merge-value": "^1.0.1"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^1.0.20",
    "@types/classnames": "^2.2.10",
    "@types/crypto-js": "^3.1.44",
    "@types/express": "^4.17.6",
    "@types/history": "^4.7.5",
    "@types/jest": "^25.2.1",
    "@types/js-cookie": "^2.2.6",
    "@types/lodash": "^4.14.150",
    "@types/qs": "^6.9.1",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/react-helmet": "^5.0.15",
    "@umijs/fabric": "^2.0.8",
    "@umijs/preset-react": "^1.4.18",
    "@umijs/preset-ui": "^2.1.13",
    "chalk": "^4.0.0",
    "check-prettier": "^1.0.3",
    "cross-env": "^7.0.2",
    "cross-port-killer": "^1.2.1",
    "enzyme": "^3.11.0",
    "express": "^4.17.1",
    "gh-pages": "^2.2.0",
    "husky": "^4.2.5",
    "import-sort-cli": "^6.0.0",
    "import-sort-parser-babylon": "^6.0.0",
    "import-sort-parser-typescript": "^6.0.0",
    "import-sort-style-module": "^6.0.0",
    "jest-puppeteer": "^4.4.0",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^10.1.7",
    "mockjs": "^1.1.0",
    "node-fetch": "^2.6.0",
    "prettier": "^2.0.5",
    "pro-download": "1.0.1",
    "stylelint": "^13.3.3"
  },
  "optionalDependencies": {
    "puppeteer": "^2.1.1"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "checkFiles": [
    "src/**/*.js*",
    "src/**/*.ts*",
    "src/**/*.less",
    "config/**/*.js*",
    "scripts/**/*.js"
  ]
}
